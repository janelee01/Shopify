{%- case icon -%}
  {%- when 'icon-magnify' -%}
    <svg viewBox="0 0 19 19">
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-magnify" x="0" y="0"></use>
    </svg>
  {%- when 'icon-airline-1' -%}
    <svg viewBox="0 0 325 310">
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-airline-1" x="0" y="0"></use>
    </svg>
  {%- when 'icon-airline-2' -%}
    <svg viewBox="0 0 325 310">
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-airline-2" x="0" y="0"></use>
    </svg>
{%- endcase -%}
