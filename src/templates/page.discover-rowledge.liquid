{% include 'vars' %}

{%- assign assetVersion = 74557 -%}
{%- assign ctaTitle = 'The Rowledge' -%}
{%- assign ctaPrice = '$375' -%}
{%- assign ctaLink = '/products/rowledge-nylon-black-gold-grey' -%}
{%- assign ctaImg = 'rowledge-cta' -%}
{%- assign ctaImgCol = 'col-md-5 col-md-pull-5' -%}
