                <source srcset="https://cdn.shopify.com/s/files/1/2185/1497/files/rowledge-video-lg.jpg?{{ assetVersion }}" media="(min-width: 768px)">
                <img src="https://cdn.shopify.com/s/files/1/2185/1497/files/rowledge-video-xs.jpg?{{ assetVersion }}" alt="Edgemont">
            </picture>
            <div class="topic-content">
                <h3 class="title">What Fits Inside</h3>
                <a href="#" data-video-id="1" class="video-trigger">
                    <img src="{{ 'play-v2.svg' | asset_url }}" alt="Play video">
                </a>
            </div>
        </div>
    </div>
    
    <a id="nav-target-3" class="jumptarget"></a>
    <div class="max-container animate" {{ animateFadeInUp }}>
        <div class="topic-block has-overlay-xs mb-gutter mb-negate-sm">
